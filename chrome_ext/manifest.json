{
  "manifest_version": 3,
  "name": "Phishing Detector",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icons.png",
      "48": "icons/icons.png",
      "128": "icons/icons.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
